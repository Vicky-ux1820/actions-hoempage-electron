{
  "name": "7_electron_hello",
  "version": "1.0.0",
  "description": "Your app description here",
  "main": "./app/main/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron": "electron .",
    "dev": "concurrently \"npm run start\" \"npm run electron\"",
    "react-electron": "concurrently \"react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "prod": "npm run build && npm run electron",
    "dist": "npm run build && electron-builder",
    "electron-builder": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.2",
    "sass-loader": "^16.0.2",
    "wait-on": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.0",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "html-webpack-plugin": "^5.6.3",
    "react-scripts": "^5.0.1",
    "sass": "^1.79.4",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "build": {
    "appId": "com.electron.reactapp",
    "productName": "Electron React App",
    "files": [
      "dist/**/*",
      "electron.js",
      "src/preload.js",
      "package.json",
      "renderer.js",
      ".eigenspace/**/*"
    ],
    "fileAssociations": [
      {
        "ext": "project-eigenspace",
        "name": "Eigenspace Project",
        "description": "Eigenspace Project File",
        "role": "Editor"
      }
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
